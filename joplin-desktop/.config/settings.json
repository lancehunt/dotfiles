{
	"$schema": "https://joplinapp.org/schema/settings.json",
	"locale": "en_US",
	"markdown.plugin.softbreaks": false,
	"markdown.plugin.typographer": false,
	"spellChecker.language": "en-US",
	"ui.layout": {
		"key": "root",
		"children": [
			{
				"key": "sideBar",
				"width": 294,
				"visible": true
			},
			{
				"key": "noteList",
				"width": 334,
				"visible": true
			},
			{
				"key": "editor",
				"visible": true
			}
		],
		"visible": true
	},
	"noteVisiblePanes": [
		"editor",
		"viewer"
	],
	"dateFormat": "MM/DD/YYYY",
	"timeFormat": "h:mm A",
	"sync.target": 3,
	"theme": 2,
	"themeAutoDetect": true,
	"style.editor.fontFamily": "",
	"windowContentZoomFactor": 110,
	"style.editor.fontSize": 16,
	"clipperServer.autoStart": true
}
